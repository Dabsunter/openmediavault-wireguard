{
  "type": "config",
  "id": "conf.service.wireguard.client",
  "title": "wireguard client",
  "queryinfo": {
    "xpath": "//services/wireguard/clients/client",
    "iterable": true,
    "idproperty": "uuid"
  },
  "properties": {
    "uuid": {
      "type": "string",
      "format": "uuidv4"
    },
    "enable": {
      "type": "boolean",
      "default": 0
    },
    "netnum": {
      "type": "integer",
      "minimum": 0,
      "maximum": 255
    },
    "name": {
      "type": "string"
    },
    "nic": {
      "type": "string"
    },
    "endpoint": {
      "type": "string"
    },
    "port": {
      "type": "integer",
      "minimum": 1,
      "maximum": 65535,
      "default": 51820
    },
    "privatekeyserver": {
      "type": "string"
    },
    "publickeyserver": {
      "type": "string"
    },
    "privatekeyclient": {
      "type": "string"
    },
    "publickeyclient": {
      "type": "string"
    },
    "presharedkeyclient": {
      "type": "string"
    }
  }
}
