{
	"type": "config",
	"id": "conf.service.wireguard",
	"title": "WireGuard",
	"queryinfo": {
		"xpath": "//services/wireguard",
		"iterable": false
	},
	"properties": {
		"enable": {
			"type": "boolean",
			"default": 0
		},
		"nic": {
			"type": "string"
		},
		"endpoint": {
			"type": "string"
		},
		"port": {
			"type": "integer",
			"minimum": 1,
			"maximum": 65535,
			"default": 51820
		},
		"privatekeyserver": {
			"type": "string"
		},
		"privatekeyclient": {
			"type": "string"
		},
		"publickeyserver": {
			"type": "string"
		},
		"publickeyclient": {
			"type": "string"
		}
	}
}
